{
    "name": "rezaamini-ir/laravel-easypanel",
    "license": "MIT",
    "description": "An admin panel based on livewire",
    "authors": [
        {
            "name": "Reza Amini",
            "email": "rezaaminiroyal@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/support": "^6.0|^7.0|^8.0|^9.0",
        "rezaamini-ir/laravel-easyblade" : "^0.5",
        "laravel/framework": "^6.0|^7.0|^8.0|^9.0",
        "livewire/livewire": "^1.0|^2.3",
        "iya30n/dynamic-acl": "^1.0.3"
    },
    "require-dev": {
        "orchestra/testbench": "~3.7|^4.0|^5.0|^6.0"
    },
    "autoload": {
        "psr-4": {
            "EasyPanel\\" : "src/"
        },
        "files": ["src/helpers.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "EasyPanelTest\\" : "tests/"
        }
    },

    "extra": {
        "laravel" : {
            "providers" : [
                "EasyPanel\\EasyPanelServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
